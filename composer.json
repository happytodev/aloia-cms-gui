{
    "name": "roelofjan-elsinga/flat-file-cms-gui",
    "description": "The GUI for the roelofjan-elsinga/flat-file-cms package",
    "require": {
        "php": ">=7.1.3",
        "roelofjan-elsinga/flat-file-cms": "^0.10.4",
        "illuminate/support": "^5.8",
        "illuminate/view": "^5.8",
        "gumlet/php-image-resize": "^1.9",
        "illuminate/http": "^5.8",
        "rbdwllr/reallysimplejwt": "^2.0",
        "genealabs/laravel-caffeine": "^0.8.3",
        "roelofjan-elsinga/flat-file-cms-publish": "^0.2.2"
    },
    "keywords": [
        "laravel",
        "cms",
        "file",
        "user interface"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roelof Jan Elsinga",
            "email": "roelofjanelsinga@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "FlatFileCms\\GUI\\": "src/",
            "FlatFileCms\\GUI\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "FlatFileCms\\GUI\\FlatFileCmsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^3.8"
    }
}
